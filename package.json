{
  "name": "@singularit/eslint-config-monorepo",
  "version": "0.0.1",
  "private": true,
  "author": "singularIT GmbH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/singularit-de/eslint-config.git"
  },
  "keywords": [],
  "main": "index.js",
  "workspaces": [
    "configs/*"
  ],
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "eslint": "^8.16.0",
    "semantic-release-monorepo": "^7.0.5",
    "typescript": "^4.7.2"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "npmPublish": false,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "dryRun": true
  },
  "publishConfig": {
    "access": "public"
  }
}
